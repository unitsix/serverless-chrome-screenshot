        "babel-register": "^6.24.1",
        "babel-preset-stage-3": "6.24.1",
        "chrome-remote-interface": "0.25.3",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "serverless": "1.24.1",
        "serverless-plugin-warmup": "^3.6.2-rc.1",
        "serverless-plugin-chrome": "1.0.0-46",
